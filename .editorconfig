# https://EditorConfig.org

root = true

[*]
charset = utf-8
indent_style = space
indent_size = 4
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.md]
trim_trailing_whitespace = false

[*.{js,json,mjs,jsx,ts,tsx}]
indent_size = 2
max_line_length = 100

[*.go]
indent_style = tab

[*.{bat,cmd}]
end_of_line = crlf
